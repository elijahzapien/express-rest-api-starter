{
  "name": "express-rest-api-starter",
  "version": "0.0.1",
  "engines": {
    "node": ">=5.0.0"
  },
  "private": true,
  "description": "Boilerplate for building a RESTful API using Express",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "babel src -d dist",
    "debug": "DEBUG='Server,Server:*'",
    "dev": "npm run build && npm run debug nodemon dist/server.js",
    "production": "npm run build && node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/elijahzapien/express-rest-api-starter.git"
  },
  "devDependencies": {},
  "dependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-module-alias": "^1.2.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.5.2",
    "body-parser": "^1.15.0",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "mongoose": "^4.4.4",
    "nodemon": "^1.9.0"
  }
}
